<%= simple_form_for @acronym do |f| %>
  <%= f.input :acry %>
  <%= f.input :full_form %>
  <%= f.input :definition %>
  <%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>

<%= form_tag import_acronyms_path, multipart: true do %>
  <%= file_field_tag :file %>
    <%= submit_tag "import csv", class: 'button' %>
    <% end %>
